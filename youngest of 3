#include <stdio.h>
int main()
{
    int a, b, c;
    printf("Enter a, b, c = ");
    scanf("%d%d%d", &a, &b, &c);

    if (a < b && a < c)  
    {
        printf("a is youngest");
    }
    else if (b < a && b < c)  
    {
        printf("b is youngest");
    }
    else  
    {
        printf("c is youngest");
    }

    return 0;
}
