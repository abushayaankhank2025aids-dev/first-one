#include <stdio.h>
int main()
{
    int a, b, c;
    printf("Enter a, b, c = ");
    scanf("%d%d%d", &a, &b, &c);

    if (a > b && a > c)   // Check if a is greatest
    {
        printf("a is greatest");
    }
    else if (b > a && b > c)   // Check if b is greatest
    {
        printf("b is greatest");
    }
    else   // Otherwise c is greatest
    {
        printf("c is greatest");
    }

    return 0;
}
