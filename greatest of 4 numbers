// Online C compiler to run C program online
#include <stdio.h>

int main() {
int a,b,c,d;
printf("enter a b c d=");
scanf("%d%d%d%d",&a,&b,&c,&d);
if(a>b && a>c && a>d)
{    
    printf("a is greater",a);
    
}
else if(a<b && b>c && b>d)
{
    printf("b is greater",b);
    
}
else if(c>b && a<c && c>d)
{
    printf("c is greater",c);
}
else{
    printf("d is greater",d);
}
}
