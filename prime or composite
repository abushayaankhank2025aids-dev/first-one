// Online C compiler to run C program online
#include <stdio.h>

int main() {
int a;
printf("enter a number:");
scanf("%d",&a);
int b=0;
for(int i=2; i<=(a-1); i++)
   {
       if(a%i==0)
       {
           b=1;
           break;
       }
   }   
if(b==1) {printf("no. is composite");}
else if (b==0) {printf("no. is prime");}
else {printf("no. is composite and prime");}
}
